/dts-v1/;

/ {
        compatible = "system-device-tree-v1";
        // this means run it first
        priority = <1>;
        lops {

                lop_0 {
                        compatible = "system-device-tree-v1,lop,load";
                        load = "assists/openamp.py";
                };
                lop_2 {
                        compatible = "system-device-tree-v1,lop,load";
                        load = "assists/openamp_xlnx.py";
                };
                lop_3 {
                        compatible = "system-device-tree-v1,lop,load";
                        load = "assists/openamp-xlnx-zynq.py";
                };
        };
};
